layout {
  match: Articles(this)
  mode (Summary)

  content {
    section {
      content {
        image-card{
          aspect-ratio(4:3)
          title-area{
            halign (Start)
           slot1 {
          text {
            
            value("[#{value(this.title)}]")
            style (Title_M)
          }
        }
          }
          image-url("[#{value(this.urlToImage)}]")
        }
       
    }
  }
}
}
